<h2 class="h2">Cadastrar Novo Usuario</h2>


<div class="container_2">
	<form method="post" action="users/userform">
		<div class="form-row">
			<div class="form-group col-md-7">
				<label for="inputName">Nome</label>
				<input type="text" id="nome" name="nome" class="form-control" required>
			</div>
			<div class="form-group col-md-7">
				<label for="inputName">Email</label>
				<input type="email" class="form-control" id="valorCusto" name="email" required>
			</div>
			<div class="form-group col-md-7">
				<label for="inputName">Senha</label>
				<input type="password" class="form-control" id="valorVenda" name="password" required>
			</div>
		</div>
		<button type="submit" class="btn btn-primary">Cadastrar</button>
	</form>
</div>